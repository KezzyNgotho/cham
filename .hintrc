{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "disown-opener": "off",
    "button-type": "off",
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ]
  }
}